print ('la la la')

# import turtle
import turtle



# defining colors
colors = ['red', 'yellow', 'green', 'purple', 'blue', 'orange']

# setup turtle pen
t= turtle.Pen()

# changes the speed of the turtle
t.speed(10)

# changes the background color
turtle.bgcolor("black")


t.pencolor('blue')
t.forward(100)
t.right(120)
t.forward(100)
t.right(120)
t.forward(100)


print ('ahoi')
turtle.done()

